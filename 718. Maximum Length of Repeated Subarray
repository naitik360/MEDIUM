class Solution {
public:
    int findLength(vector<int>& s1, vector<int>& s2) {
        int n1=s1.size(),n2=s2.size(),ans=INT_MIN;
        
        int dp[n1+1][n2+1];
        for(int i=0;i<=n1;i++)
        {
            for(int j=0;j<=n2;j++)
            {
                if(i==0 || j==0)
                     dp[i][j]=0;
                else if(s1[i-1]==s2[j-1])
                    dp[i][j]=dp[i-1][j-1]+1;
                else
                    dp[i][j]=0;
                
                ans=max(ans,dp[i][j]);
            }
        }
      return ans;
    }
};
